#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/vasantsaladi/Documents/GitHub/GhostType-AI/client/node_modules/.pnpm/publish-browser-extension@3.0.0/node_modules/publish-browser-extension/bin/node_modules:/Users/vasantsaladi/Documents/GitHub/GhostType-AI/client/node_modules/.pnpm/publish-browser-extension@3.0.0/node_modules/publish-browser-extension/node_modules:/Users/vasantsaladi/Documents/GitHub/GhostType-AI/client/node_modules/.pnpm/publish-browser-extension@3.0.0/node_modules:/Users/vasantsaladi/Documents/GitHub/GhostType-AI/client/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/vasantsaladi/Documents/GitHub/GhostType-AI/client/node_modules/.pnpm/publish-browser-extension@3.0.0/node_modules/publish-browser-extension/bin/node_modules:/Users/vasantsaladi/Documents/GitHub/GhostType-AI/client/node_modules/.pnpm/publish-browser-extension@3.0.0/node_modules/publish-browser-extension/node_modules:/Users/vasantsaladi/Documents/GitHub/GhostType-AI/client/node_modules/.pnpm/publish-browser-extension@3.0.0/node_modules:/Users/vasantsaladi/Documents/GitHub/GhostType-AI/client/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../publish-browser-extension/bin/publish-extension.cjs" "$@"
else
  exec node  "$basedir/../publish-browser-extension/bin/publish-extension.cjs" "$@"
fi
