#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/vasantsaladi/Documents/GitHub/GhostType-AI/client/node_modules/.pnpm/wxt@0.19.27_@types+node@22.13.4_rollup@4.34.8/node_modules/wxt/bin/node_modules:/Users/vasantsaladi/Documents/GitHub/GhostType-AI/client/node_modules/.pnpm/wxt@0.19.27_@types+node@22.13.4_rollup@4.34.8/node_modules/wxt/node_modules:/Users/vasantsaladi/Documents/GitHub/GhostType-AI/client/node_modules/.pnpm/wxt@0.19.27_@types+node@22.13.4_rollup@4.34.8/node_modules:/Users/vasantsaladi/Documents/GitHub/GhostType-AI/client/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/vasantsaladi/Documents/GitHub/GhostType-AI/client/node_modules/.pnpm/wxt@0.19.27_@types+node@22.13.4_rollup@4.34.8/node_modules/wxt/bin/node_modules:/Users/vasantsaladi/Documents/GitHub/GhostType-AI/client/node_modules/.pnpm/wxt@0.19.27_@types+node@22.13.4_rollup@4.34.8/node_modules/wxt/node_modules:/Users/vasantsaladi/Documents/GitHub/GhostType-AI/client/node_modules/.pnpm/wxt@0.19.27_@types+node@22.13.4_rollup@4.34.8/node_modules:/Users/vasantsaladi/Documents/GitHub/GhostType-AI/client/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/wxt.mjs" "$@"
else
  exec node  "$basedir/../../bin/wxt.mjs" "$@"
fi
